<%= simple_form_for(@egc_server_type) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.association :quote_request_item_server %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

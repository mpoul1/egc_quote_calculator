<%- model_class = QuoteRequestItemServer -%>
<div class="page-header">
  <h1><%=t '.title', :default => QuoteRequestItemServer %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:required_cpu) %></dt>
      <dd><pre class="prettyprint"><%= @quote_request_item_server.required_cpu %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:required_ram) %></dt>
      <dd><pre class="prettyprint"><%= @quote_request_item_server.required_ram %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              quote_request_item_servers_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_quote_request_item_server_path(@quote_request_item_server), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              quote_request_item_server_path(@quote_request_item_server),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>


<h1>Listing quote_requests</h1>

<table>
  <thead>
    <tr>
      <th>Project</th>
      <th>Customer</th>
      <th>Requested at</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quote_requests.each do |quote_request| %>
      <tr>
        <td><%= quote_request.project %></td>
        <td><%= quote_request.customer %></td>
        <td><%= quote_request.requested_at %></td>
        <td><%= quote_request.description %></td>
        <td><%= link_to 'Show', quote_request %></td>
        <td><%= link_to 'Edit', edit_quote_request_path(quote_request) %></td>
        <td><%= link_to 'Destroy', quote_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quote request', new_quote_request_path %>

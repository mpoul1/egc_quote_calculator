<h1>Listing hw_types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Egc server type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hw_types.each do |hw_type| %>
      <tr>
        <td><%= hw_type.name %></td>
        <td><%= hw_type.description %></td>
        <td><%= hw_type.egc_server_type %></td>
        <td><%= link_to 'Show', hw_type %></td>
        <td><%= link_to 'Edit', edit_hw_type_path(hw_type) %></td>
        <td><%= link_to 'Destroy', hw_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Hw type', new_hw_type_path %>

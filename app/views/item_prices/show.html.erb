<%- model_class = ItemPrice -%>
<div class="page-header">
  <h1><%=t '.title', :default => ItemPrice %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:valid_from) %></dt>
      <dd><pre class="prettyprint"><%= @item_price.valid_from %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:valid_to) %></dt>
      <dd><pre class="prettyprint"><%= @item_price.valid_to %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:valid_price) %></dt>
      <dd><pre class="prettyprint"><%= @item_price.valid_price %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              item_prices_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_item_price_path(@item_price), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              item_price_path(@item_price),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>


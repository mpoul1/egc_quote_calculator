<%- model_class = CustomerTaxZone -%>
<div class="page-header">
  <h1><%=t '.title', :default => CustomerTaxZone %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:name) %></dt>
      <dd><pre class="prettyprint"><%= @customer_tax_zone.name %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:description) %></dt>
      <dd><pre class="prettyprint"><%= @customer_tax_zone.description %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:tax) %></dt>
      <dd><pre class="prettyprint"><%= @customer_tax_zone.tax %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              customer_tax_zones_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_customer_tax_zone_path(@customer_tax_zone), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              customer_tax_zone_path(@customer_tax_zone),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>


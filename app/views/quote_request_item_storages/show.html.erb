<%- model_class = QuoteRequestItemStorage -%>
<div class="page-header">
  <h1><%=t '.title', :default => QuoteRequestItemStorage %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:name) %></dt>
      <dd><pre class="prettyprint"><%= @quote_request_item_storage.name %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:description) %></dt>
      <dd><pre class="prettyprint"><%= @quote_request_item_storage.description %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:capacity) %></dt>
      <dd><pre class="prettyprint"><%= @quote_request_item_storage.capacity %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:backup_size) %></dt>
      <dd><pre class="prettyprint"><%= @quote_request_item_storage.backup_size %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              quote_request_item_storages_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_quote_request_item_storage_path(@quote_request_item_storage), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              quote_request_item_storage_path(@quote_request_item_storage),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

